{
  "name": "replicache-todo",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}'"
  },
  "engines": {
    "//": "replicache-nextjs requires npm >= 7. This was released in Node 15, but 16 is current stable so we choose that. Render looks at package.json to determine which Node to use: https://render.com/docs/node-version.",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "^12.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "replicache": "^12.0.1",
    "replicache-nextjs": "^0.5.1",
    "replicache-react": "^2.10.0",
    "todomvc-app-css": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.11",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "plugins": [
      "react"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": true,
    "tabWidth": 2,
    "useTabs": false
  }
}
